<script>
function run(thaiCode) {
	f=new fopen("./~tmp.htma", "w");
	f.write("<script\n>"+thaiCode+"</script\n>");
	f.close();
	cmd=new commands;
	print("Shell's Return : "+cmd.getoutput('thaipe $PWD/~tmp.htma'));
}

function handleEvent(keyEvent) {
	if (keyEvent.keyCode==118)
		exec();
}
_num=0;
function exec() {
	try{
		var obj=document.createElement('textarea');
		if (_num==0) {
			$('#resultScreen')[0].value=" -*- THAI PE Shell 0.1 -*- :";
			_num++;
		}
		obj.value=$('#resultScreen')[0].value+"\n>>>"+eval( $('#shell')[0].value);	
		$("#resultPlace")[0].removeChild($('#resultScreen')[0]);
		obj.setAttribute("id", "resultScreen" );
		obj.rows=10;
		obj.cols=50;
		$("#resultPlace")[0].appendChild(obj);
		obj.scrollTop = obj.scrollHeight; 

		}
	catch(e){
		print('JavaScript Error :     '+e);
		}	
}
</script>
